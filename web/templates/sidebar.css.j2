.grid {
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  display: flex;
  overflow-y: hidden;
}

.grid-cell {
  overflow-y: hidden;
}

tr {
  background: #242424 !important;
  border: 1px solid #444444 !important;
}

td {
  background: #242424 !important;
  border: 1px solid #444444 !important;
  padding: 0px 0px 0px 5px !important;
  width: 50%;
}

.indent {
  margin-left: 6px !important;
  margin-right: 6px !important;
}

#sidebar-container {
  flex: 0 1 400px;
  min-width: 400px;
  -webkit-flex-wrap: nowrap;
  -ms-flex-wrap: nowrap;
  flex-wrap: nowrap;
  background: #000000;
}

#sidebar {
  position: absolute;
  padding: 0px;
  margin-bottom: 5px;
  background: #242424;
  min-width: 400px;
  width: 100%;
  height: 100%;
  overflow-y: auto !important;
}

.table-responsive {
  background: #242424 !important;
  border: 0px !important;
  width: 400px !important;
  overflow-y: auto !important;
}

.treegrid-node {
  background: #242424 !important;
  border: 0px !important;
}

#sidebar-key {
  word-wrap: break-word !important;
}

#sidebar-value {
}
