html, body {
  overflow: hidden;
  height: 100%;
  width: 100%;
  margin: 0px;
  padding: 0px;
  color: #A4A4A4;
  font-size: 14px;
  font-family: Verdana, Geneva, sans-serif;
  background: #000000;
  outline: none !important;
}

a:link,
a:visited,
a:hover,
a:active {
  text-decoration: none;
}

